for h in "necropolis" "baszerr.org"
do
  if ping -c 2 -i 0.2 -w 2 "$h" > /dev/null 2>&1
  then
    HOST="$h"
    break
  fi
done
if [ ! "$HOST" ]
then
  echo "no suitable host found" >&2
  exit 62
fi

PARENT="bzr+ssh://$HOST/var/lib/prjs/el-bart/REPO_NAME"
#PARENT="/tmp/bzr_playground/parent"

function build_project
{
  for p in debug release test mtest profile doc
  do
    make PROFILE="$p" || return $?
  done
  return 0
}
